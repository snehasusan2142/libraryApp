<div class="container">
  <div class="row">
  
            <div class="container login-container">
                <div class="row">
                  <div class="col-lg-2 l"> </div>
                    <div class="col-lg-8 centered login-form-1">
                        <h3>Sign Up</h3>
                        <form >
                    
                            <div class="form-row" id="myForm">
                                <div class="form-group col-md-6">
                                  <label for="formGroupExampleInput">First name</label>
                                  <input type="text" class="form-control" class="formGroupExampleInput" placeholder="First Name">
                                </div>
                                <div class="form-group col-md-6">
                                  <label for="formGroupExampleInput">Second name</label>
                                  <input type="text" class="form-control" class="formGroupExampleInput2" placeholder="Second Name">
                                </div>
                              </div>
                
                            <div class="form-row">
                              <div class="form-group col-md-6">
                                <label for="inputEmail4">Email</label>
                                <input type="email" class="form-control" id="inputEmail4" onkeyup="ValidateEmail()">
                                <span id="error1"></span> 
                              </div>
                              <div class="form-group col-md-6">
                                <label for="inputPassword4" >Phone number</label>
                                <input type="text" class="form-control" id="inputPhone" onkeyup="phonenumber()">
                                <span id="error2"></span> 
                              
                              </div>
                            </div>
                 
                            <div class="form-group">
                            
                                         <label for="passwordInput" class="col-md-12  control-label"> <b>Password</b> </label>
                                         <div class="col-md-12">
                                          <input type="password" class="form-control" id="password" name ="passwordInput" placeholder="Enter your Password" onkeyup="checkstrenght();CheckPassword()" >
                                          <span id="error3"></span> 
                                          <input type="checkbox" onclick="myFunction()" id="myInputcheck">  Show password </input>
                                          <div id="popover-password">
                                            <p><span id="result"></span></p>
                                            <div class="progress">
                                              <div class="progress-bar" id="password-strength" role="progressbar" aria-valuemax="100" aria-valuemin="0" aria-valuenow="40" style="width: 0%;"></div>
                                              
                                             </div>

                                          </div>
                                        
                                          <ul class="list-unstyled">
                                           
                                            <li>
                                              <span class="low-upper-case">
                                                <i class="fas fas1 fa-circle" aria-hidden="true"></i> Lowercase & Uppercase
                                              </span>
                                            </li>
                
                                            <li>
                                              <span class="one-number">
                                                <i class="fas  fas2 fa-circle" aria-hidden="true"></i> Number(0-9)
                                              </span>
                                            </li>
                
                
                                            <li>
                                              <span class="one-special-char">
                                                <i class="fas fas3 fa-circle" aria-hidden="true"></i> Special character(!@$%&*_)
                                            </li>
                
                
                                            <li>
                                              <span class="eight-character">
                                                <i class="fas fas4 fa-circle" aria-hidden="true"></i> Atleast 8 character
                                              </span>
                                            </li>
                
                
                
                
                                          </ul>
                                        </div>
                                        
                                            
                                         <label for="passwordInput" class="col-md-12  control-label"> <b> Confirm Password</b> </label>
                                         
                                         <div class="col-md-12">
                                          <input type="password" class="form-control" id="conf" name ="passwordInput" placeholder="Enter your Password" onkeyup="confirmpass()" >
                                          <span id="error4"></span> 
                
                                         </div>
                     
                            <br>
                            
                             <div >
                               <label for="formGroupExampleInput" id="strengthresult"></label>
                             </div>
                           
                            <br>
                            <br>
                            <!-- <div class="form-group">
