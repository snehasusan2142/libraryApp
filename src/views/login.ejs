                            <div class="form-group">
                                <input type="submit" class="btnSubmit" value="Login" onclick="return!!(CheckPassword()&phonenumber()&ValidateEmail())"/>
                            </div>
                            <div class="form-group">
                                <a href="#" class="ForgetPwd">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Forget Password?</a>
                            </div>
                        </form>
                        <a href="signin"  class="ForgetPwd">Already a member? Click here</a>
